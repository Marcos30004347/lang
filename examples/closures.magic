fun2 :: (x: i32) -> i32 -> i32 {
	return (y: i32) -> i32 {
		return x + y;
	}
}

fun1 :: (x: i32) -> i32 -> i32 {
	fun2 :: (y: i32) -> i32 {
		return x + y + 3;
	}
	
	return fun2;
}

fun3 :: (x: i32) -> i32 {
	return fun1(x)(4);
}

fun4 :: (x: i32) -> i32 -> i32 {
	z : i32 : 0;
	i : i32 = 0;
	
	fun5 :: (w: i32) -> i32 {
		i = i + 1;

		fun6 :: (u:i32) -> i32 {
			z = z + 1;

			return u*z + w;	
		}

		return fun6;
	}
	
	c := fun5(6);

	z = z + 4;

	return c;
}


